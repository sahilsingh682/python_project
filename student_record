import pandas as pd
import numpy as np
from random import shuffle

marks = np.arange(61,101)
shuffle(marks)
marks = marks.reshape(8,5)
print(marks)

names =['sahil','uttam','prince','annay','rahul','amit','ankit']
subjects = ['hindi','english','physics','chemistry','maths']

grade = pd.DataFrame(data= marks,index=names,columns =subjects)
grade